{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.8" }
    }
  ],
  "hooks": {
    "build": "apt-get update && apt-get install -y libxml2-dev libxslt-dev"
  }
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.py"
    }
  ]
}
