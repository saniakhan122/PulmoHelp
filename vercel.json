{
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.py"
    }
  ],
  "build": {
    "commands": [
      "apt-get update",
      "apt-get install -y libxml2-dev libxslt-dev",
      "pip install -r requirements.txt"
    ]
  }
}
